{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2024,
    "sourceType": "module"
  },
  "globals": {
    "allQuestions": "readonly",
    "questions": "readonly",
    "new_questions_batch1": "readonly",
    "new_questions_batch2": "readonly",
    "new_questions_batch3": "readonly",
    "new_questions_batch4": "readonly",
    "new_questions_batch5": "readonly"
  },
  "rules": {
    "no-unused-vars": "off"
  }
}
